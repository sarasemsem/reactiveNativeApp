{
  "name": "seddik-s-auto",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "expo lint"
  },
  "dependencies": {
    "@expo/config-plugins": "~6.0.0",
    "@expo/vector-icons": "^13.0.0",
    "@react-native-async-storage/async-storage": "1.17.11",
    "@react-native-community/cli-platform-android": "^18.0.0",
    "@react-native-picker/picker": "2.4.8",
    "@react-navigation/bottom-tabs": "^6.5.20",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/native-stack": "^6.9.12",
    "@types/react": "~18.0.27",
    "ajv": "^8.12.0",
    "ajv-keywords": "^5.1.0",
    "date-fns": "^4.1.0",
    "expo": "~50.0.0",
    "expo-blur": "~12.2.2",
    "expo-constants": "~14.2.1",
    "expo-dev-client": "~2.2.1",
    "expo-font": "~11.1.1",
    "expo-haptics": "~12.2.1",
    "expo-image": "~1.0.2",
    "expo-image-picker": "~14.1.1",
    "expo-json-utils": "^0.15.0",
    "expo-linking": "~4.0.1",
    "expo-print": "~12.2.1",
    "expo-router": "^3.5.0",
    "expo-splash-screen": "~0.18.2",
    "expo-status-bar": "~1.4.2",
    "expo-symbols": "~0.4.4",
    "expo-system-ui": "~2.2.1",
    "expo-web-browser": "~12.1.1",
    "firebase": "^11.9.1",
    "lodash": "^4.17.21",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.71.14",
    "react-native-animatable": "^1.4.0",
    "react-native-gesture-handler": "~2.9.0",
    "react-native-get-random-values": "~1.9.0",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-pager-view": "6.1.2",
    "react-native-paper": "^5.14.1",
    "react-native-reanimated": "~2.14.4",
    "react-native-safe-area-context": "4.5.0",
    "react-native-screens": "~3.20.0",
    "react-native-select-dropdown": "^4.0.1",
    "react-native-swipe-list-view": "^3.2.9",
    "react-native-tab-view": "^4.1.2",
    "react-native-vector-icons": "^10.2.0",
    "react-native-web": "~0.18.9",
    "react-native-webview": "11.26.0",
    "typescript": "^4.9.4",
    "zustand": "^5.0.4",
    "expo-modules-autolinking": "~1.1.0 || ~1.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@expo/metro-config": "^0.1.16",
    "@expo/ngrok": "^4.1.3",
    "@react-native-community/cli": "latest",
    "@react-native/gradle-plugin": "^0.79.0",
    "@types/react": "~18.0.27",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~9.2.0",
    "metro": "^0.76.9",
    "metro-babel-transformer": "^0.71.0",
    "metro-cache": "^0.71.0",
    "metro-config": "^0.71.0",
    "metro-core": "^0.71.0",
    "metro-react-native-babel-preset": "^0.77.0",
    "metro-resolver": "^0.71.0",
    "metro-runtime": "^0.71.0",
    "metro-source-map": "^0.71.0",
    "metro-symbolicate": "^0.71.0",
    "metro-transform-plugins": "^0.71.0",
    "metro-transform-worker": "^0.71.0",
    "typescript": "~5.8.3"
  },
  "private": true,
  "expo": {
    "name": "seddik-service-auto",
    "slug": "seddik-service-auto",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "scheme": "seddiksauto",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sarasemsem.seddiksauto"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.sarasemsem.seddiksauto"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {}
    },
    "owner": "sarasemsem"
  },
  "doctor": {
    "reactNativeDirectoryCheck": {
      "exclude": [
        "ajv",
        "ajv-keywords",
        "firebase",
        "lodash",
        "react-native-select-dropdown",
        "react-native-vector-icons"
      ]
    }
  },
  "expoDoctor": {
    "reactNativeDirectoryCheck": {
      "exclude": [
        "ajv",
        "ajv-keywords",
        "firebase",
        "lodash",
        "react-native-select-dropdown",
        "react-native-swipe-list-view",
        "react-native-vector-icons"
      ],
      "listUnknownPackages": false
    }
  }
}
